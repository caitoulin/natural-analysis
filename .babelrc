{
  "presets": ["@babel/preset-env", "@babel/preset-react"],// preset-env只会编译当前环境不支持的语法
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": true,
        "corejs": false,
        "regenerator": true,
        "useESModules": true,
        "absoluteRuntime": false,
        "version": "7.0.0-beta.0"
      }
    ],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}
